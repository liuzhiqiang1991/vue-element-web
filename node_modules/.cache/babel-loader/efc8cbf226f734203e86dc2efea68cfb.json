AqBY,QAArB,CAA8BV,GAA9B,CAEE,UAAAU,QAAQ,EAAI;AACV,MAAMC,GAAG,GAAGD,QAAQ,CAACE,IAArB;;AAEA,MAAID,GAAG,CAACE,IAAJ,KAAa,CAAjB,EAAoB;AAAE;AACpBzB,IAAAA,OAAO,CAAC;AACN0B,MAAAA,OAAO,EAAEH,GAAG,CAACG,OAAJ,IAAe,cADlB;AAENC,MAAAA,IAAI,EAAE,OAFA;AAGNC,MAAAA,QAAQ,EAAE,IAAI;AAHR,KAAD,CAAP,CADkB,CAOlB;;AACA,QAAIL,GAAG,CAACE,IAAJ,KAAa,KAAb,IAAsBF,GAAG,CAACE,IAAJ,KAAa,KAAnC,IAA4CF,GAAG,CAACE,IAAJ,KAAa,KAA7D,EAAoE;AAClE;AACA1B,MAAAA,UAAU,CAAC8B,OAAX,CAAmB,gFAAnB,EAAqG,gBAArG,EAAuH;AACrHC,QAAAA,iBAAiB,EAAE,UADkG;AAErHC,QAAAA,gBAAgB,EAAE,QAFmG;AAGrHJ,QAAAA,IAAI,EAAE;AAH+G,OAAvH,EAIGK,IAJH,CAIQ,YAAM;AACZ/B,QAAAA,KAAK,CAACgC,QAAN,CAAe,iBAAf,EAAkCD,IAAlC,CAAuC,YAAM;AAC3CE,UAAAA,QAAQ,CAACC,MAAT;AACD,SAFD;AAGD,OARD;AASD;;AACD,WAAOf,OAAO,CAACC,MAAR,CAAe,IAAIe,KAAJ,CAAUb,GAAG,CAACG,OAAJ,IAAe,MAAzB,CAAf,CAAP;AACD,GArBD,MAqBO;AACL,WAAOH,GAAP;AACD;AACF,CA7BH,EA8BE,UAAAN,KAAK,EAAI;AACPC,EAAAA,OAAO,CAACC,GAAR,CAAY,QAAQF,KAApB,EADO,CACoB;;AAC3B,MAAQoB,GAAR,GAAgBpB,KAAK,CAACK,QAAN,CAAeE,IAA/B,CAAQa,GAAR;AAEArC,EAAAA,OAAO,CAAC;AACN0B,IAAAA,OAAO,EAAEW,GAAG,IAAI,MADV;AACkB;AACxBV,IAAAA,IAAI,EAAE,OAFA;AAGNC,IAAAA,QAAQ,EAAE,IAAI;AAHR,GAAD,CAAP;AAKA,SAAOR,OAAO,CAACC,MAAR,CAAeJ,KAAf,CAAP;AACD,CAxCH;AA2CA,eAAed,OAAf","sourcesContent":["import axios from 'axios'\nimport { MessageBox, Message } from 'element-ui'\nimport store from '@/store'\nimport { getToken } from '@/utils/auth'\n\n// create an axios instance\nconst service = axios.create({\n  baseURL: process.env.VUE_APP_BASE_API, // url = base url + request url\n  // withCredentials: true, // send cookies when cross-domain requests\n  timeout: 5000 // request timeout\n})\n\n// request interceptor\nservice.interceptors.request.use(\n  config => {\n    if (store.getters.token) {\n      config.headers['X-Token'] = getToken()\n      config.headers['Authorization'] = `Bearer ${getToken()}` // 这里相当于把token赋予了浏览器\n    }\n    return config\n  },\n  error => {\n    // do something with request error\n    console.log(error) // for debug\n    return Promise.reject(error)\n  }\n)\n\n// response interceptor\nservice.interceptors.response.use(\n\n  response => {\n    const res = response.data\n\n    if (res.code !== 0) { // 不等于0表示异常，后端自定义\n      Message({\n        message: res.message || '服务端返回非0，请求失败',\n        type: 'error',\n        duration: 5 * 1000\n      })\n\n      // 50008: Illegal token; 50012: Other clients logged in; 50014: Token expired;\n      if (res.code === 50008 || res.code === 50012 || res.code === 50014) {\n        // to re-login\n        MessageBox.confirm('You have been logged out, you can cancel to stay on this page, or log in again', 'Confirm logout', {\n          confirmButtonText: 'Re-Login',\n          cancelButtonText: 'Cancel',\n          type: 'warning'\n        }).then(() => {\n          store.dispatch('user/resetToken').then(() => {\n            location.reload()\n          })\n        })\n      }\n      return Promise.reject(new Error(res.message || '请求失败'))\n    } else {\n      return res\n    }\n  },\n  error => {\n    console.log('err' + error) // for debug\n    const { msg } = error.response.data\n\n    Message({\n      message: msg || '请求失败', // 把后端传递的信息返给前端\n      type: 'error',\n      duration: 5 * 1000\n    })\n    return Promise.reject(error)\n  }\n)\n\nexport default service\n"]}]}