  <el-label>点2经度：</el-label>\n\n      <el-input\n        v-model=\"lon2\"\n        placeholder=\"点2经度\"\n        style=\"width: 200px\"\n        class=\"filter-item\"\n        clearable\n      />\n      <el-label>    点2纬度：</el-label>\n\n      <el-input\n        v-model=\"lat2\"\n        placeholder=\"点2纬度\"\n        style=\"width: 200px\"\n        class=\"filter-item\"\n        clearable\n      />\n\n      <el-button\n        v-waves\n        class=\"filter-item\"\n        type=\"primary\"\n        icon=\"el-icon-search\"\n        style=\"margin-left: 10px\"\n        @click=\"handle\"\n      >计算\n      </el-button>\n    </div>\n\n  </div>\n</template>\n\n<script>\n\nexport default {\n  filters: {\n\n  },\n  data() {\n    return {\n      lon1: 120.0,\n      lat1: 70.0,\n      lon2: 120.1,\n      lat2: 70.1\n    }\n  },\n  methods: {\n\n    handle() {\n      // alert(this.lon1)\n      var lat1 = this.lat1 * 3.14 / 180.0\n      var lat2 = this.lat2 * 3.14 / 180.0\n      var a = lat1 - lat2\n      var b = this.lon1 * 3.14 / 180.0 - this.lon2 * 3.14 / 180.0\n      var s = 2 * Math.asin(Math.sqrt(Math.pow(Math.sin(a / 2), 2) + Math.cos(lat1) * Math.cos(lat2) * Math.pow(Math.sin(b / 2), 2)))\n      s = s * 6378.137\n      // s = Math.round(s * 10000) / 10000;\n      alert(s + '公里')\n    },\n    clear() {\n      this.lon1 = ''\n      this.lat1 = ''\n      this.lon2 = ''\n      this.lat2 = ''\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n</style>\n"]}]}